{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": ["prettier"],
    "rules": {
        "indent": ["error", 4],
        "no-param-reassign": "off",
        "import/prefer-default-export": "off",
        "import/no-extraneous-dependencies": "off",
        "prettier/prettier": "error"
    }
}
